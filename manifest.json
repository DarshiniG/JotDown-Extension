{

"name":"JotDown",
"description":"Jot down notes which is integrated with web page usage time tracker.",
"version":"1.0",

"manifest_version":2,

"browser_action": {

 "default_popup":"popup.html",
 "default_icon": {
      "16": "icon.png",
      "19": "browser19.png",
      "38": "browser38.png" 
    }
},

 "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
},

"permissions": [
 "management",
 "cookies",
 "tabs"
   
],

"background": {
    "scripts": [
      "common.js",
      "background.js"
      ],
    "persistent": true
  },
  
"options_page": "options.html"






}
